{
  "macros": ["NFC_I2C_SDA=GPS_NFC_I2C_SDA","NFC_I2C_SCL=GPS_NFC_I2C_SCL","NFC_FIELD_DETECTION=NFC_FIELD_DETECT"
],
"target_overrides": {
